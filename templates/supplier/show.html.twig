{% extends 'base.html.twig' %}

{% block title %}Supplier{% endblock %}

{% block body %}
    <h1>Supplier</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ supplier.id }}</td>
            </tr>
            <tr>
                <th>Supplier_name</th>
                <td>{{ supplier.supplierName }}</td>
            </tr>
            <tr>
                <th>Contact_person</th>
                <td>{{ supplier.contactPerson }}</td>
